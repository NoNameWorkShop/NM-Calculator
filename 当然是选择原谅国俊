{
	
	int month;
	
	printf("1代表子正，2代表为文，3代表怡然，4代表俊杰，5代表国俊 \n"); 
	scanf("%d", &month);
	
	
	if (month == 3) 
	 printf("此人是个萌妹子", month);
 	else if(month == 2 || month == 1 )
 	 printf("此人是个菊苣");
 	  	else if(month == 4)
 		 printf("此人是辣鸡", month);
 	  		else if(month == 5)
 		 	printf("此人是傻子 \n此人是傻子 \n此人是傻子 \n因为很重要所以说三遍 \n", month);
 		 		else
				  printf("假的，不存在的。（逃 \a \n"); 	
    return 0;
    
}
